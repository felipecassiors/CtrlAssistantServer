<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
  <children>
      <StackPane VBox.vgrow="ALWAYS">
         <children>
        <Label alignment="CENTER" style="&#10;" text="CtrlClass" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </StackPane>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Label alignment="CENTER_RIGHT" text="Comunicação" />
          <Button fx:id="startCommunicationButton" mnemonicParsing="false" onAction="#startCommunicationButtonPressed" text="Iniciar" />
            <Button fx:id="stopCommunicationButton" mnemonicParsing="false" onAction="#stopCommunicationButtonPressed" text="Parar" />
            <Label alignment="CENTER_RIGHT" text="Aula" />
            <Button fx:id="startClassButton" defaultButton="true" mnemonicParsing="false" onAction="#startClassButtonPressed" text="Iniciar" />
            <Button fx:id="finishClassButton" mnemonicParsing="false" onAction="#finishClassButtonPressed" text="Terminar" />
            <Button fx:id="generateReportsButton" mnemonicParsing="false" onAction="#generateReportsButtonPressed" text="Relatórios" />
        </buttons>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </ButtonBar>
  </children>
</VBox>
